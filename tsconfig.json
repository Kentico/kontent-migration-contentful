{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": ["es6"],
    "allowJs": false,
    "declaration":false,
    "rootDir": "src",
    "outDir": "src",                         
    "strict": true,   
    "noImplicitAny": true,   
    "esModuleInterop": true,           
    "resolveJsonModule": true,     
    "forceConsistentCasingInFileNames": true 
  },
  "exclude":[
    "build"
  ]
}
